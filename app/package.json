{
  "name": "@rete-tacticum/game-decipher",
  "version": "1.0.5",
  "type": "module",
  "description": "Decipher game for blackshell terminal project",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "url": "git+https://github.com/rete-tacticum/game_decipher.git"
  },
  "main": "./dist/game-decipher.umd.cjs",
  "module": "./dist/game-decipher.js",
  "exports": {
    ".": {
      "import": "./dist/game-decipher.js",
      "require": "./dist/game-decipher.umd.cjs"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "scripts": {
    "dev": "vite --host=0.0.0.0 --port=3000",
    "build": "tsc && vite build",
    "lint": "eslint . --ext js,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint . --ext js,ts,tsx --report-unused-disable-directives --fix",
    "preview": "vite preview --host=0.0.0.0 --port=3111",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "nanoid": "^4.0.0",
    "react-error-boundary": "^4.0.13"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.2",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "identity-obj-proxy": "^3.0.0",
    "nanoid": "^5.0.7",
    "postcss": "^8.4.16",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.1",
    "postcss-minify": "^1.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-scss": "^4.0.4",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "use-debounce": "^10.0.0",
    "vite": "^5.1.6",
    "vitest": "^1.4.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}